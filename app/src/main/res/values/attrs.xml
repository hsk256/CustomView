<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Options">
        <attr name="titleText" format="string" localization="suggested"/>
        <attr name="valueColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="float_menu">
        <attr name="margin_left" format="integer"/>
        <attr name="margin_right" format="dimension"/>
        <attr name="margin_bottom" format="dimension"/>
        <attr name="position" format="enum">
            <enum name="left_bottom" value="0"/>
            <enum name="right_bottom" value="1"/>
        </attr>
    </declare-styleable>




    <declare-styleable name="ArcMenu">
        <attr name="position1" >

        </attr>
        <attr name="radius" format="dimension"/>
    </declare-styleable>
    <attr name="position1" >
        <enum name="left_top" value="0"></enum>
        <enum name="left_bottom" value="1"></enum>
        <enum name="right_top" value="2"></enum>
        <enum name="right_bottom" value="3"></enum>
    </attr>

    <declare-styleable name="CustomTitleView">
        <attr name="titleTextCustom" />
        <attr name="titleColor"/>
        <attr name="titleSize" />
    </declare-styleable>

    <attr name="titleTextCustom" format="string"></attr>
    <attr name="titleColor" format="color"/>
    <attr name="titleSize" format="dimension"/>


    <declare-styleable name="CustomProgressView" >
        <attr name="firstColor"/>
        <attr name="secondColor"/>
        <attr name="width"/>
        <attr name="speed"/>
    </declare-styleable>

    <attr name="firstColor" format="color"/>
    <attr name="secondColor" format="color"/>
    <attr name="width" format="dimension"/>
    <attr name="speed" format="integer"/>
</resources>
