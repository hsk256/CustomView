<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Options">
        <attr name="titleText" format="string" localization="suggested"/>
        <attr name="valueColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="float_menu">
        <attr name="margin_left" format="integer"/>
        <attr name="margin_right" format="dimension"/>
        <attr name="margin_bottom" format="dimension"/>
        <attr name="position" format="enum">
            <enum name="left_bottom" value="0"/>
            <enum name="right_bottom" value="1"/>
        </attr>
    </declare-styleable>


</resources>
